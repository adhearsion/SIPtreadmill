- content_for(:page_icon) { "icon-rocket" }
- content_for(:page_title) { "Editing Test Run" }
= render "layouts/page_header"
#page_test_run.container-fluid.padded
  .row-fluid
    .span12
      .box
        .box-header
          %span.title Edit Test Run
          %ul.box-toolbar
            %li= link_to '', :back, class: 'btn btn-primary icon-arrow-left', title: 'Back'
            %li= link_to '', @test_run, class:'btn btn-info icon-eye-open', title: 'Show'
        .box-content.padded-top
          = render 'form'

